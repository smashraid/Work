@{
    Layout = "_Section.cshtml";
    string view = Request.QueryString["view"];
    string gymnastId = Request.QueryString["gymnastid"];
    string memberId = Request.QueryString["memberid"];
    string chatId = Request.QueryString["chatid"];
    string chatTitle = Request.QueryString["chattitle"];
    string topicId = Request.QueryString["topicid"];
    string topicTitle = Request.QueryString["topictitle"];
    string workoutId = Request.QueryString["workoutid"];
    Dictionary<string, string> model = new Dictionary<string, string>
        {
            {"gymnastid", gymnastId},
            {"memberid", memberId},
            {"chatid", chatId},
            {"chattitle", chatTitle},
            {"workoutid", workoutId},
            {"topicid", topicId},
            {"topictitle", topicTitle}
        };
}
@switch (view)
{
    case "gymnast":
        @Html.Partial("_Client", model)
        break;
    case "measurement":
        @Html.Partial("_Measurement", model)
        break;
    case "workout":
        @Html.Partial("_Workout", model)
        break;
    case "chat":
        @Html.Partial("_Chat", model)
        break;
    case "talk":
        @Html.Partial("_Talk", model)
        break;
    case "topic":
        @Html.Partial("_Topic", model)
        break;
    case "post":
        @Html.Partial("_Post", model)
        break;
    case "routine":
        @Html.Partial("_Routine", model)
        break;
    case "superset":
        @Html.Partial("_Topic")
        break;
}
